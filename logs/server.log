                // User is not logged in
                userStatusDiv.innerHTML = `
                    <a href="/login" class="auth-button">Login</a>
                    <a href="/register" class="auth-button">Register</a>
                `;
                console.log('User not logged in or API error:', error.message);
            });
    }
});

